{
  "name": "opera-bach-ui",
  "version": "2.0.0",
  "description": "Bach-UI for OPERA",
  "main": "index.js",
  "jest": {
    "transform": {
      "^.+\\.svg$": "jest-svg-transformer",
      "^.+\\.(js|jsx)$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    },
    "moduleNameMapper": {
      "@bach/test-utils": "<rootDir>/src/test_utils.jsx",
      "@bach/(.*)": "<rootDir>/src/$1",
      "^.+.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.js"
    ]
  },
  "scripts": {
    "test": "jest --silent",
    "lint": "eslint src --ext .jsx",
    "lint:js": "eslint src --ext .js",
    "start": "webpack-dev-server --open --mode development",
    "build": "webpack --mode production",
    "prod": "webpack-dev-server --open --mode production",
    "start2": "webpack-dev-server --config ./webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nasa/opera-sds-bach-ui.git"
  },
  "author": "benjamin.poreh@jpl.nasa.gov",
  "license": "ISC",
